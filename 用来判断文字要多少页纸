package wangpingyu;

import java.util.Scanner;

public class rtret {
	public static void main(String args[]) {
		Scanner scn=new Scanner(System.in);
       int a=scn.nextInt();
       int b=scn.nextInt();
       int c;
       c=a/b;
       if(a%b!=0)
       {
    	   c+=1;
       }
       System.out.println(c);
}
}
